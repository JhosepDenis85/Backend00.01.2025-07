Proceso Ejercicio23
	//23. Hacer un algoritmo en Pseint para calcular la suma de los números impares menores o iguales a n.
	Escribir "Ingrese un número n:"
	Leer n
	suma <- 0

	Para i <- 1 Hasta n Con Paso 2
		suma <- suma + i
	FinPara

	Escribir "Suma de impares menores o iguales a ", n, ": ", suma
FinProceso
